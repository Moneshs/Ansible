                               

server {
  listen 80;
  server_name _;
  location = /favicon.ico { access_log off; log_not_found off; }
  location /static/ {
      root /home/ubuntu/ChatApplication/chatapp/chat;
  }
  location / {
      include proxy_params;
      proxy_pass http://unix:/home/ubuntu/ChatApplication/chatapp/chat/chatapp.sock;
  }
}

