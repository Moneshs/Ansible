[Unit]
Description=gunicorn daemon
After=network.target
[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/ChatApplication/chatapp/chat
ExecStart=/home/ubuntu/ChatApplication/venv/bin/gunicorn --workers 3 --bind unix:/home/ubuntu/ChatApplication/chatapp/chat/chatapp.sock chatapp.wsgi:application
[Install]
WantedBy=multi-user.target

